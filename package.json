  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rosskevin/yup.git"
  },
  "author": "@rosskevin Kevin Ross",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rosskevin/yup/issues"
  },
  "homepage": "https://github.com/rosskevin/yup",
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@4c/rollout": "^1.2.0",
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^23.3.11",
    "@types/lodash": "^4.14.119",
    "@types/rollup": "^0.54.0",
    "@types/sinon": "^7.0.3",
    "benchmark": "^2.1.4",
    "doctoc": "^1.4.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rollup": "^1.0.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "sinon": "^7.2.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "fn-name": "~2.0.1",
    "lodash": "^4.17.11",
    "synchronous-promise": "^2.0.6",
    "toposort": "^2.0.2"
  }
